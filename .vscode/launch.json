{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "gh-vstest-logger",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "dotnet build",
            "logging": {
                "programOutput": true,
                "processExit": true,
                "moduleLoad": false,
            },
            "program": "dotnet",
            "args": [
                "test",
                "--nologo",
                "--no-build",
                "--logger",
                "\"github;name=unit-tests;CI=1;GITHUB_ACTIONS=1;echo=1\""
            ],
            "cwd": "${workspaceFolder}",
            "justMyCode": true,
            "stopAtEntry": false,
            "console": "internalConsole",
            "internalConsoleOptions": "openOnFirstSessionStart",
            "env": {
                "VSTEST_HOST_DEBUG": "1"
            }
        },
        {
            "name": "attach testhost",
            "type": "coreclr",
            "request": "attach",
            "processName": "testhost",
            "logging": {
                "programOutput": true,
                "processExit": true,
                "moduleLoad": false,
            },
        },
    ]
}